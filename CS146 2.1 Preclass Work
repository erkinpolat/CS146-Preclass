     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 576x432 with 1 Axes>"
      ]
     },
     "execution_count": 24,
     "metadata": {
      "image/png": {
       "height": 370,
       "width": 503
      }
     },
     "output_type": "execute_result"
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 576x432 with 1 Axes>"
      ]
     },
     "execution_count": 24,
     "metadata": {
      "image/png": {
       "height": 370,
       "width": 510
      }
     },
     "output_type": "execute_result"
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 576x432 with 1 Axes>"
      ]
     },
     "execution_count": 24,
     "metadata": {
      "image/png": {
       "height": 370,
       "width": 510
      }
     },
     "output_type": "execute_result"
    }
   ],
   "source": [
    "#I define p initially as the prior distribution, after each step I'll use the posterior as prior. So I'll write it in p.\n",
    "p = prior\n",
    "\n",
    "\n",
    "#I create a for loop so that I can enter the data one by one. In each step I use the previous posterior distribution as the prior distribution and I muliply by the new likelihood.    \n",
    "\n",
    "\n",
    "for i in range(0, 4):\n",
    "    \n",
    "# Creating the likelihoods from the data\n",
    "    sigma = 4\n",
    "    lower_limit = 0\n",
    "    upper_limit = np.inf\n",
    "    likelihood = sts.truncnorm.pdf(\n",
    "        data[i],\n",
    "        (lower_limit-mu)/sigma,  # See the SciPy documentation. The lower and upper\n",
    "        (upper_limit-mu)/sigma,  # limits are defined in terms of the standard normal.\n",
    "        loc=mu,\n",
    "        scale=sigma)\n",
    "    \n",
    "    #unnormalized posterior distribution is calculated \n",
    "    unnormalized_post=p*likelihood\n",
    "    \n",
    "    #Finding the area for normalization\n",
    "    area = sp.integrate.trapz(unnormalized_post, mu)\n",
    "    post=unnormalized_post/area\n",
    "    \n",
    "    #New posterior distribution is wrritten as the prior\n",
    "    p=post\n",
    "    \n",
    "    #I plot a revised graph after every step. First one is the same as before since the first data point is 20\n",
    "    plt.figure(figsize=(8, 6))\n",
    "    plt.plot(mu, likelihood, color='red', label='likelihood')\n",
    "    plt.plot(mu, prior, color='black', linestyle=':', label='prior')\n",
    "    plt.plot(mu, post, color='black', label='posterior')\n",
    "    plt.xlabel('')\n",
    "    plt.ylabel('probability density')\n",
    "    plt.legend()\n",
    "\n",
    "    \n",
    "\n",
    "\n",
    "    "
   ]
  },
